{
  "expo": {
    "name": "DevTales",
    "slug": "DevTales",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Devtales.png",
    "scheme": "devtales",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Devtales.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Devtales.png",
        "backgroundColor": "#000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nemisruparel.DevTales"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Devtales.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c18f4a0b-33a5-43cc-9843-6080a044845c"
      }
    }
  }
}
